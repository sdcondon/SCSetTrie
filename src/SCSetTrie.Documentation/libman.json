{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.9.1",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@highlightjs/cdn-assets@11.8.0",
      "destination": "wwwroot/lib/highlightjs/",
      "files": [
        "highlight.js",
        "highlight.min.js",
        "LICENSE",
        "package.json",
        "README.md",
        "languages/csharp.min.js",
        "styles/default.min.css"
      ]
    }
  ]
}